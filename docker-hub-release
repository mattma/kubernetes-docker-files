#!/bin/bash

set -o errexit
set -o nounset
set -o pipefail

if [ ! -f settings ]; then
  echo "Error loading configuration; settings file missing."
  exit 1
fi

source ./settings

# for b in kube-apiserver kube-proxy kube-scheduler kube-controller-manager kubelet kubectl; do
for b in kube-apiserver kube-scheduler kube-controller-manager kube-proxy; do
  docker tag -f ${b}:${KUBERNETES_VERSION} ${DOCKER_REGISTRY_USERNAME}/${b}:${KUBERNETES_VERSION}
  docker push ${DOCKER_REGISTRY_USERNAME}/${b}:${KUBERNETES_VERSION}
done
